{
  "name": "tender-project-management",
  "version": "1.0.0",
  "description": "Система управления проектом разработки AI-ассистента для тендеров",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "init-db": "node server.js --init",
    "backup": "sqlite3 tender_project.db .dump > backup.sql",
    "restore": "sqlite3 tender_project.db < backup.sql"
  },
  "keywords": [
    "project-management",
    "tender",
    "sqlite",
    "nodejs",
    "express"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
